<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 数据库驱动包位置 -->
    <classPathEntry
            location="/Users/admin/.m2/repository/mysql/mysql-connector-java/5.1.34/mysql-connector-java-5.1.34.jar"/>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>
        <!-- 数据库链接URL、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost/wee"
                        userId="root" password="123456">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!--生成model相关-->
        <javaModelGenerator targetPackage="com.wenkang.wee.api.entity"
                            targetProject="/Users/admin/Documents/javaproject/wenakang-wee/wee-api/src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaModelGenerator>

        <!--生成mapper.xml相关-->
        <sqlMapGenerator targetPackage="mapper"
                         targetProject="/Users/admin/Documents/javaproject/wenakang-wee/wee-dao/src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <!--生成mapper相关-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.wenkang.wee.dao"
                             targetProject="/Users/admin/Documents/javaproject/wenakang-wee/wee-dao/src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <table tableName="t_" domainObjectName="" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"/>

    </context>
</generatorConfiguration>